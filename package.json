{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.2.0",
  "scripts": {
    "build": "vite-ssg build",
    "dev": "vite --port 3333",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "setup": "npx simple-git-hooks",
    "sizecheck": "npx vite-bundle-visualizer",
    "pre-commit": "pnpm lint-staged && pnpm vue-tsc --noEmit",
    "encrypt": "node -e 'require(\"./src/utils/encryption.cjs\").encryptJSONFile()'",
    "decrypt": "node -e 'require(\"./src/utils/encryption.cjs\").decryptJSONFile()'"
  },
  "dependencies": {
    "@egjs/vue3-flicking": "^4.10.7",
    "@unocss/eslint-config": "^0.51.4",
    "@unocss/reset": "^0.51.3",
    "@vue-macros/reactivity-transform": "^0.3.4",
    "@vueuse/core": "^9.13.0",
    "@vueuse/head": "^1.1.23",
    "@vueuse/integrations": "^9.13.0",
    "crypto-js": "^4.1.1",
    "leaflet": "^1.9.3",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.34",
    "primeicons": "^6.0.1",
    "primevue": "^3.26.1",
    "universal-cookie": "^4.0.4",
    "useCookies": "link:@vueuse/integrations/useCookies",
    "vue": "^3.2.47",
    "vue-demi": "^0.13.11",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@iconify-json/ci": "^1.1.9",
    "@iconify-json/fa6-solid": "^1.1.13",
    "@iconify-json/ic": "^1.1.13",
    "@iconify-json/ph": "^1.1.5",
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@types/leaflet": "^1.9.3",
    "@types/nprogress": "^0.2.0",
    "@unocss/preset-attributify": "^0.50.8",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue-macros/volar": "^0.9.5",
    "@vue/test-utils": "^2.3.2",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "cypress": "^12.9.0",
    "cypress-vite": "^1.3.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "eslint-plugin-cypress": "^2.13.2",
    "https-localhost": "^4.7.1",
    "lint-staged": "^13.2.1",
    "pnpm": "^8.2.0",
    "shiki": "^0.14.1",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.9.1",
    "typescript": "^4.9.5",
    "unocss": "^0.50.8",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-vue-components": "^0.24.1",
    "unplugin-vue-macros": "^2.0.0",
    "vite": "^4.2.1",
    "vite-bundle-visualizer": "^0.6.0",
    "vite-plugin-inspect": "^0.7.20",
    "vite-plugin-pages": "^0.29.0",
    "vite-plugin-pwa": "^0.14.7",
    "vite-plugin-vue-inspector": "^3.4.0",
    "vite-plugin-vue-markdown": "^0.22.4",
    "vite-plugin-webfont-dl": "^3.7.3",
    "vite-ssg": "^0.22.2",
    "vite-ssg-sitemap": "^0.4.3",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.3.14"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm pre-commit"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
  }
}
